<openerp>
	<data>

		<record id="rgpd_info_partner_form" model="ir.ui.view">
			<field name="name">rgpd.info.partner.form</field>
			<field name="model">res.partner</field>
			<field name="inherit_id" ref="base.view_partner_form"/>
			<field name="arch" type="xml">
				<xpath expr="//page[@name='internal_notes']" position="after">
					<page string="RGPD Information">
						<group string="RGPD Information">
							<field name="response_rgpd"/>
							<field name="date_response"/>
						</group>
					</page>
				</xpath>
			</field>
		</record>

	</data>
</openerp>
